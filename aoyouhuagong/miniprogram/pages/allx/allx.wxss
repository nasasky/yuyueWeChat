.scroll-view_H {
  white-space: nowrap;
}

.scroll-view-item-img {
  width: 100%;
  height: 100%;
}

.scroll-view-item_H {
  display: inline-block;
  width: 100%;
  height: 350rpx;
}

.view-container {
  background-color: #fff;
}

.data-loading {
  height: 100rpx;
  line-height: 100rpx;
  background-color: #eee;
  text-align: center;
  font-size: 14px;
}

.articles-container {
  border-bottom: 1px solid #eee;
  margin: 30rpx 10rpx;
  /* background-color: #eee;   */
}

.articles-container .info {
  font-size: 12px;
  margin: 20rpx 0rpx;
  height: 100%;
  display: inline-block;
}

.articles-container .info .avatar {
  width: 100rpx;
  height: 100rpx;
  margin-right: 10rpx;
  border-radius: 100rpx;
  display: inline-block;
  vertical-align: middle;
}

.articles-container .info .created-at {
  /* margin: 0px 10rpx;   */
  display: inline-block;
  vertical-align: middle;
}

.avatar {
  float: left;
  margin-top: 40rpx;
}

.created-at {
  float: left;
  width: 70%;
  font-size: 30rpx;
  margin-top: 10rpx;
}

.category {
  float: left;
  width: 70%;
  font-size: 30rpx;
  margin-top: 10rpx;
}

.articles-container .info .category {
  color: #3399ea;
  display: inline-block;
  vertical-align: middle;
}

.faqix {
  float: left;
  margin-left: 2.5%;
  height: 50px;
  border-bottom: 1px solid #ebebeb;
  margin-top: 10px;
  width: 95%;
}

.huodong {
  font-size: 18px;
  line-height: 40px;
  margin-left: 10px;
  margin-bottom: 10px;
}

.imgd {
  float: left;
  width: 40px;
  height: 40px;
  margin-bottom: 10px;
}

.imgdx {
  float: right;
  width: 40px;
  height: 40px;
  margin-bottom: 10px;
}
